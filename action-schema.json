      },
      "folderId": {
        "type": "integer", 
        "required": true,
        "description": "UiPath Folder ID for scoping"
      },
      "path1": {
        "type": "string",
        "required": false,
        "description": "Path to first document in bucket"
      },
      "path2": {
        "type": "string",
        "required": false,
        "description": "Path to second document in bucket"
      },
      "path3": {
        "type": "string",
